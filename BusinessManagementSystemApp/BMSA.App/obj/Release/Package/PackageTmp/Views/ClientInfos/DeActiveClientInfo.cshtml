
@{
    ViewBag.Title = "DeActiveClientInfo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-offset-1 col-md-10 col-sm-offset-1 col-sm-10">
        <div class="panel panel-info">
            <div class="panel-heading">DeActive User List</div>
            <div class="panel-body">
                <div class="col-md-12 col-sm-12">
                    <table id="activeClientInfoHistory" class="table table-bordered table-hover display">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>PhoneNo</th>
                                <th>Address</th>
                                <th>Is Active</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</div>

@section ViewSpecificJavascript
{
    <script src="~/Scripts/MilkManagement/ClientInfos/deActiveClientInfoForm.js"></script>
}
